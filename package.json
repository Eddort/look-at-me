{
  "name": "ts-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "test": "jest",
    "start": "echo 'print you start command'\n\n",
    "dev": "npm-run-all --parallel build:watch start",
    "build": "tsc",
    "build:watch": "tsc --watch --preserveWatchOutput"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eddort/ts-boilerplate.git"
  },
  "keywords": [
    "ts",
    "typescript",
    "boilerplate",
    "ts-boilerplate"
  ],
  "author": "Eddort",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Eddort/ts-boilerplate/issues"
  },
  "homepage": "https://github.com/Eddort/ts-boilerplate#readme",
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/jest": "^23.3.0",
    "jest": "^23.4.1",
    "npm-run-all": "^4.1.3",
    "ts-jest": "^23.0.0",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testRegex": "./__tests__/.*.spec.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "@types/node": "^10.5.2",
    "commander": "^2.16.0",
    "dotenv": "^6.0.0"
  }
